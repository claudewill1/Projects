@model int
@{ 
    ViewBag.Title = "404 Error";
}
<div class="alert alert-danger mt-1 mb-1">
    <h4>404 Not found error:</h4>
    <h5>Book with Id = @Model cannot be found.</h5>
</div>
<a asp-action="index" asp-controller="home" class="btn btn-outline-success">Click here to see a list of all books.</a>